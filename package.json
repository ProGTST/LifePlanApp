{
  "name": "life-plan-gant",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint src",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format": "prettier --write \"src/**/*.{css,ts,html,json}\"",
    "dev": "node scripts/generate-custom-icons-json.js && vite",
    "dev:host": "node scripts/generate-custom-icons-json.js && vite --host",
    "build": "node scripts/generate-custom-icons-json.js && tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.mjs",
    "tauri": "set \"CARGO_BUILD_JOBS=1\" && set \"PATH=%USERPROFILE%\\.cargo\\bin;%PATH%\" && tauri"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "chart.js": "^4.5.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "fastify": "^5.7.4"
  },
  "devDependencies": {
    "@eslint/js": "^9",
    "@tauri-apps/cli": "^2",
    "autoprefixer": "^10.4.27",
    "eslint": "^9",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "stylelint": "^16.15.0",
    "stylelint-config-recommended": "^14.0.0",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8",
    "vite": "^6.0.3"
  }
}
